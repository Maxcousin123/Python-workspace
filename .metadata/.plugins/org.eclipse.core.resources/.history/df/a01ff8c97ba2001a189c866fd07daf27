class patient:
    
    def setid(self,id):
        self.setidd=id
        
    def getid(self,):
        return self.id
    
    def setname(self,name):
        self.setname=name
        
    def getname(self):
        return self.name
    
    def setssn(self,ssn):
        self.setssn=ssn
        
    def getssn(self):
        return self.ssn
    
p=patient()
p.setidd(123)
p.setssn(456)
p.setname('mohammed')

print(p.getidd())
print(p.getname())
print(p.getssn())
