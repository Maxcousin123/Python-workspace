class patient:
    
    def setidd(self,idd):
        self.setidd=idd
    def getidd(self):
        return self.idd
    def setname(self,name):
        self.setname=name
    def getname(self):
        return self.name
    def setssn(self,ssn):
        self.setssn=ssn
    def getssn(self):
        return self.ssn
    
p=patient()

p.setssn()
p.setname()
print(p.getidd())
print(p.getname())
print(p.getssn())
